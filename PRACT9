x <- c(132,129,120,113.2,105,92,84,83.2,88.4,59,80,81.5,71,69.2)
y<- c(46,48,51,52.1,54,52,59,58.7,61.6,64,61.4,54.6,58.8,58)
corr<-cor(x,y)
desvx<-sd(x)
desvy<-sd(y)
medx<-mean(x)
medy<-mean(y)
B1<-corr*(desvy/desvx)
B0<-medy-B1*medx
base1 <-data.frame(x,y)
library(foreign)
library(ggplot2)
library(psych)

plot(base1[,1], base1[,2], main=" Diagrama de DispersiÃ³n",xlab = "X",ylab = "Y")
abline(lm(base1[,2]~base1[,1]), col="darkblue")
